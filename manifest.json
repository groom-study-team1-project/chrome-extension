{
  "manifest_version": 3,
  "name": "Goorm DeepDive Duplicate Detection",
  "description": "구름 딥다이브 수강생들을 위한 중복강의 감지 확장프로그램",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://k-digital.goorm.io/*", "https://k-digital.goorm.io/?"],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "scripts/global/helpers.js"
      ]
    }, {
      "matches": ["https://k-digital.goorm.io/learn/lecture/51540/10%ED%9A%8C%EC%B0%A8-%EC%9E%90%EB%B0%94-%EC%8A%A4%ED%94%84%EB%A7%81-%EB%A6%AC%EC%95%A1%ED%8A%B8-%ED%92%80%EC%8A%A4%ED%83%9D-%EA%B0%9C%EB%B0%9C%EC%9E%90-%EC%84%B1%EC%9E%A5-%EA%B3%BC%EC%A0%95"],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "scripts/main.js",
        "scripts/lecture/db/storage.js",
        "scripts/lecture/controller/Controller.js",
        "scripts/lecture/model/Lecture.js",
        "scripts/lecture/views/View.js",
        "scripts/lecture/views/LectureListView.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon.png"
  }
}
